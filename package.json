{
  "name": "reactcomponents-guru",
  "version": "1.0.0",
  "description": "Bundles all the onboarding components per channel and keep the bundle to be exported in build folder.",
  "keywords": [
    "react",
    "components",
    "reusableComponents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gurusubramaniam/reactComponents.git"
  },
  "scripts": {
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "sematic-release": "semantic-release --no-cli",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.6",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-core": "^6.6.5",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4npm ",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "filemanager-webpack-plugin": "^1.0.26",
    "husky": "^4.2.5",
    "jest": "^23.6.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-test-renderer": "^16.6.0",
    "semantic-release": "^19.0.3",
    "webpack": "^4.20.2 ",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.12"
  }
}
