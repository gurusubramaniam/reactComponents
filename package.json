{
  "name": "portableonboarding",
  "version": "1.0.0",
  "description": "Bundles all the onboarding components per channel and keep the bundle to be exported in build folder.",
  "keywords": [
    "react",
    "components",
    "reusableComponents"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.paypal.com:ssubramaniam/ProgressiveBundler.git"
  },
  "scripts": {
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.full.js && webpack --config webpack.config.js",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4npm ",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-check-es2015-constants": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "filemanager-webpack-plugin": "^1.0.26",
    "jest": "^23.6.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-test-renderer": "^16.6.0",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^2.0.15"
  }
}
